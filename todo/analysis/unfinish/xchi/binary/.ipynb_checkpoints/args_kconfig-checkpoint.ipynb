   "source": [
    "import pandas as pd\n",
    "df1 = pd.read_csv(\"args_centroids.csv\")\n",
    "nc = len(df1.index)\n",
    "mx = 5960\n",
    "\n",
    "flist = []\n",
    "for i in range(0,mx):\n",
    "    p = i % nc\n",
    "    flist.append(list(df1.iloc[p]))\n",
    "\n",
    "df2 = pd.DataFrame(flist, columns=df1.columns)\n",
    "df2.to_csv(\"args_kconfig.csv\",index=False)"
   ]
